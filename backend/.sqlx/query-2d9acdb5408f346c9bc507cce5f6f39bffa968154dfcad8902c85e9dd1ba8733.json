{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            e.id,\n            e.class_id,\n            c.name as class_name,\n            e.display_name,\n            e.attributes,\n            e.created_at,\n            e.updated_at\n        FROM entities e\n        JOIN classes c ON c.id = e.class_id\n        WHERE e.id = $1\n          AND e.deleted_at IS NULL\n        ",
